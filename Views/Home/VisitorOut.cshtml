@{
    ViewData["Title"] = "Visitor Check Out";
}


<div class="text-center">
    <h1 class="display-4">@ViewData["Title"]</h1>

    <div class="row">
        <div class="col-sm-3"></div>
        <div class="col-sm-6" style="border: 0px solid red;">
            <label for="email">* Contact No. <span class="text-muted">(please enter contact no. which you used to register in today)</span></label>
            <input type="email" class="form-control" id="email" placeholder="phone no.">
            <div class="invalid-feedback">
                Please enter a valid Contact No. to Check Out.
            </div>
        </div>
        <div class="col-sm-3"></div>
    </div>
    <div>&nbsp;</div>
    <div class="row">
        <div class="col-sm-3"></div>
        <div class="col-sm-6" style="border: 0px solid red;">
            <button type="button" class="btn btn-primary btn-lg" onclick="location.href='@Url.Action("CheckOut", "Home")'">Check Out</button>
            <button type="button" class="btn btn-secondary btn-lg" onclick="location.href='@Url.Action("Index", "Home")'">Back to Home Page</button>
        </div>
        <div class="col-sm-3"></div>
    </div> 
   
</div>
<div>&nbsp;</div>
<div class="text-center">
    <p>This is our eVisitor Registration System. (TPL)</p>
</div>